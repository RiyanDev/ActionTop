<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
          package="tabian.com.actionbar">
    <uses-permission android:name="android.permission.INTERNET"/>
	
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="tabian.com.actionbar.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name="tabian.com.actionbar.com.myazaria.app.activity.ActivityOne"></activity>
        <activity android:name="tabian.com.actionbar.com.myazaria.app.activity.ActivityTwo"></activity>
	<activity android:name="tabian.com.actionbar.com.myazaria.app.activity.ActivityStore"></activity>
        <activity android:name="tabian.com.actionbar.com.myazaria.app.activity.ActivityThree"></activity>
        <activity android:name="tabian.com.actionbar.com.myazaria.app.activity.ActivityFour"></activity>
	<activity android:name="tabian.com.actionbar.com.myazaria.app.activity.ActivityKeluar"></activity>
        
    </application>

</manifest>
